                        if (PermitirVisualizacionTrasladosVisuTrasPen == 1) {
                            menu += `<li>
                                    <a href="/Usuario/Listado">VISUALIZACION DE TRASLADOS PENDIENTES</a>
                                </li>`;
                        }
                        menu += `</ul>
                        </li>`;
                    }
                    if (PermitirReportes == 1) {
                        menu += `<li>
                            <a class='accordion-toggle1' href='#'>
                                <span class='fa fa-file-text-o'></span>
                                <span class='sidebar-title'>REPORTES</span>
                                <span class='caret'></span>
                            </a>
                            <ul class='nav sub-nav'>`;
                        if (PermitirReportesHistorico == 1) {
                            menu += `<li>
                                    <a href="/Usuario/Listado">HISTORICO DE ARTICULO</a>
                                </li>`;
                        }
                        if (PermitirReportesEstadoRequerimientos == 1) {
                            menu += `<li>
                                    <a href="/Usuario/Listado">ESTADO DE LOS REQUERIMIENTOS</a>
                                </li>`;
                        }
                        menu += `</ul>
                        </li>`;
                    }
                    $("#ulMenuPr").html(menu);
                    configurarTogle();

                });


            }


            function configurarTogle() {
                $('.sidebar-menu li a.accordion-toggle1').on('click', function (e) {
                    e.preventDefault();

                    // If selected menu item is minified and is a submenu (has sub-nav parent) - return
                    if ($('body').hasClass('sb-l-m') && !$(this).parents('ul.sub-nav').length) { return; }

                    // If selected menu item is a dropdown - open it
                    if (!$(this).parents('ul.sub-nav').length) {

                        // If sidebar horizontal - return
                        if ($(window).width() > 900) {
                            if ($('body.sb-top').length) { return; }
                        }

                        $('a.accordion-toggle1.menu-open').next('ul').slideUp('fast', 'swing', function () {
                            $(this).attr('style', '').prev().removeClass('menu-open');
                        });
                    }
                    // If selected menu item is a dropdown inside of a dropdown -
                    // close menu items which are not children of the main top level menu
                    else {
                        var activeMenu = $(this).next('ul.sub-nav');
                        var siblingMenu = $(this).parent().siblings('li').children('a.accordion-toggle1.menu-open').next('ul.sub-nav');

                        activeMenu.slideUp('fast', 'swing', function () {
                            $(this).attr('style', '').prev().removeClass('menu-open');
                        });
                        siblingMenu.slideUp('fast', 'swing', function () {
                            $(this).attr('style', '').prev().removeClass('menu-open');
                        });
                    }

                    // Expand target menu item, add ".open-menu" class
                    // and remove unneeded inline jQuery animation styles
                    if (!$(this).hasClass('menu-open')) {
                        $(this).next('ul').slideToggle('fast', 'swing', function () {
                            $(this).attr('style', '').prev().toggleClass('menu-open');
                        });
                    }

                });

            }

        </script>

