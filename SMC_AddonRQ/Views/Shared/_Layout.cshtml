                    let PermitirReportes = 0;
                    let PermitirReportesHistorico = 0;
                    let PermitirReportesEstadoRequerimientos = 0;

                    let accesos = JSON.parse(data);
                    for (var i = 0; i < accesos.length; i++) {
                        if (accesos[i].Menu == "SEGURIDAD") { PermitirSerguridad = 1; }
                        if (accesos[i].Menu == "USUARIOS") { PermitirSeguridadUsuarios = 1; }
